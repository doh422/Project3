<% if( game && game.length > 0) { %>
	<table>
		<tbody>
			<tr>
				<td>Time</td>
				<td>Player 1</td>
				<td>Player 2</td>
				<td>Player 3</td>
				<td>Player 4</td>
			</tr>
		<% for(var i=0; i < game.length; i++) { %>	

		<tr>
		    <td><%= i+1 %>:00 pm&nbsp;</td>

			    <% for (var j = 0; j < game[i].user_ids.length; j ++) { %>
			    	<td id="userName"><%= game[i].user_ids[j].name %></td>
			    <% } %>
		    
		    <td></td>
		    <td></td>
		    <td></td>
			<td><a href="/group/add/<%= game[i].id %>" id="add">Add</a></td>

		</tr>
		<% } %>
		
		</tbody>
	</table>
<% } %>
